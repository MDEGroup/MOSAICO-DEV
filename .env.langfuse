# ============================
# Langfuse Local .env
# ============================
# Used by docker-compose.langfuse.yml

# --- Postgres ---
POSTGRES_USER=langfuse
POSTGRES_PASSWORD=langfuse
POSTGRES_DB=langfuse
POSTGRES_PORT=5432

# --- Pgvector for mosaico-app ---
PGVECTOR_USER=mosaico
PGVECTOR_PASSWORD=mosaico
PGVECTOR_DB=mosaico_db

# --- Langfuse App ---
LANGFUSE_INIT_USER_EMAIL=admin@mosaico.local
LANGFUSE_INIT_USER_PASSWORD=mosaico2025

# Public/Private keys for ingestion API
LANGFUSE_PUBLIC_KEY=pk-lf-41f76ff4-f423-4b8c-a3b7-87c5b3012015
LANGFUSE_SECRET_KEY=sk-lf-bd30b103-9a1b-43a0-88f3-742fbe657dee
LANGFUSE_ENCRYPTION_KEY=2f5d1c7a6e934b1d8c2049f6a1e3b587c4d2a9e8f1b0c3d4e5f60718293ab4cd

# App port mapping
LANGFUSE_PORT=3000
LANGFUSE_WORKER_PORT=3030

# Node options
NODE_ENV=production

# Healthcheck timeout seconds
HEALTHCHECK_START_PERIOD=15s

# --- ClickHouse ---
CLICKHOUSE_USER=langfuse_app
CLICKHOUSE_PASSWORD=lf_clickhouse_2025!
CLICKHOUSE_DB=langfuse
CLICKHOUSE_PORT=8123
CLICKHOUSE_NATIVE_PORT=9000

# --- Redis ---
REDIS_PASSWORD=lf_redis_2025!
REDIS_PORT=6379

# --- MinIO / S3 ---
MINIO_ROOT_USER=langfuse_minio
MINIO_ROOT_PASSWORD=lf_minio_2025!
MINIO_PORT=9090
MINIO_CONSOLE_PORT=9091
LANGFUSE_S3_EVENT_UPLOAD_BUCKET=langfuse-events
LANGFUSE_S3_EVENT_UPLOAD_REGION=auto
LANGFUSE_S3_EVENT_UPLOAD_PREFIX=events/
LANGFUSE_S3_MEDIA_UPLOAD_BUCKET=langfuse-media
LANGFUSE_S3_MEDIA_UPLOAD_REGION=auto
LANGFUSE_S3_MEDIA_UPLOAD_PREFIX=media/
LANGFUSE_S3_BATCH_EXPORT_ENABLED=false
LANGFUSE_S3_BATCH_EXPORT_BUCKET=langfuse-exports
LANGFUSE_S3_BATCH_EXPORT_REGION=auto
LANGFUSE_S3_BATCH_EXPORT_PREFIX=exports/
