# ============================
# Langfuse Local .env
# ============================
# Used by docker-compose.langfuse.yml

# --- Postgres ---
POSTGRES_USER=langfuse
POSTGRES_PASSWORD=langfuse
POSTGRES_DB=langfuse
POSTGRES_PORT=5432

# --- Pgvector for mosaico-app ---
PGVECTOR_USER=mosaico
PGVECTOR_PASSWORD=mosaico
PGVECTOR_DB=mosaico_db

# --- Langfuse App ---
LANGFUSE_INIT_USER_EMAIL=admin@mosaico.local
LANGFUSE_INIT_USER_PASSWORD=mosaico2025

# Public/Private keys for ingestion API
LANGFUSE_PUBLIC_KEY=pk-mosaico-local
LANGFUSE_SECRET_KEY=sk-mosaico-secret

# App port mapping
LANGFUSE_PORT=3000

# Node options
NODE_ENV=production

# Healthcheck timeout seconds
HEALTHCHECK_START_PERIOD=15s
