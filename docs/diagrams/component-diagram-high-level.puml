@startuml
package "MOSAICO Repository Components" {

  [AI Agent Repository]
  [Metadata and Taxonomy]
  [KPI and Quality Models]
  [Benchmarking Engine]
  [Search and Recommendation API]

  [Langfuse Telemetry\n(Execution Traces and Metrics)]
}

[AI Agent Repository] --> [Metadata and Taxonomy]
[AI Agent Repository] --> [Benchmarking Engine]
[Benchmarking Engine] --> [KPI and Quality Models]

[Benchmarking Engine] --> [Langfuse Telemetry\n(Execution Traces and Metrics)]
[AI Agent Repository] --> [Langfuse Telemetry\n(Execution Traces and Metrics)]

[Search and Recommendation API] --> [AI Agent Repository]
[Search and Recommendation API] --> [KPI and Quality Models]

@enduml
@startuml
package "MOSAICO Repository" {
  [AI Agent Repository]
  [Benchmarking Engine]
  [Search and Recommendation API]
}

package "Observability" {
  [Langfuse Telemetry]
}

[AI Agent Repository] --> [Benchmarking Engine]
[Benchmarking Engine] --> [Langfuse Telemetry]
[AI Agent Repository] --> [Langfuse Telemetry]
